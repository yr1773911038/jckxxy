/*新闻中心开始*/
/*新闻轮转图片开始*/
.imageRotation{
	height:380px;
	width:780px;
	overflow:hidden;  /*--超出容器的所有元素都不可见--*/
	position:relative;/*--相对定位--*/
	float:none;
	bodrer-radius:5px;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	z-index: 100;
	}
/*-------------图片容器---------------*/
.imageBox{
	position:absolute;  /*--固定定位--*/
	height:380px;
	top:0px;
	left:0px;
	overflow:hidden;
	}
.imageBox img {
	display:block;
	height:380px;
	width:780px;
	float:left;
	border:none;
	}
/*-------------标题容器---------------*/
.titleBox{
	position:absolute;
	bottom:0px;
	width:780px;
	height:40px;
	overflow:hidden;
	}
.titleBox p{
	position:absolute;
	bottom:-40px;
	width:770px;
	height:40px;
	margin:0px;
	padding:0px 10px;
	line-height:45px;
	z-index:1;
	border-top:1px solid #000;
	background-color:#000;
	color: #FFF;
	font-weight: bold;
	opacity:0.5;
	-moz-opacity:0.5;
	-webkit-opacity:0.5;
	filter:alpha(opacity=50);
	font-size:14px;
	}
.titleBox p span{
	opacity:1;
	-moz-opacity:1;
	-webkit-opacity:1;
	filter:alpha(opacity=100);
	}
.titleBox p.active{
	bottom:0px;
	}
/*-------------图标容器---------------*/
.icoBox{
	position:absolute;  /*--固定定位--*/
	bottom:10px;
	right:15px;
	height:12px;
	text-align:center;
	line-height:45px;
	z-index:2;
	}
.icoBox span{
	display:block;
	float:left;
	height:7px;
	width:12px;
	margin-left:3px;
	overflow:hidden;
	background:#B2AFA5;
	border:solid 1px #DEDEDE;
	cursor:pointer;
	}
.icoBox span.active {
	background-position:0px -12px;
	cursor:default;
	background:#F66E08;
	}
/*新闻轮转图片结束*/
