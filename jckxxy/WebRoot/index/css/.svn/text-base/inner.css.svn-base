/*banner图的设置*/
.content .channeltopimg { width:100%;display:block;margin:0 auto;}
.relative { position:relative;} 
.banner-content { position:absolute; z-index:1000; bottom:0px ; right:20px; width:405px; height:253px;  background:url(../images/banner-bg.png) repeat scroll 0 0; color:#ffffff;overflow:hidden;}
.banner-content h3 {
	font-weight: bold;
	border-bottom: solid 1px #d6f3ff;
	color: #FFF;
	padding: 0px 10px;
	font-size: 38px;
	}
.banner-content  p { 
	font-size: 14px;
	padding: 0px 10px;
	line-height: 22px;
	text-align: left;
	height: 173px;
	overflow: hidden;
}
.logo_img{
	background: url(../images/hd_logo.png) no-repeat center;
    width: 100%;
    height: 115px;
    margin: auto;
}













